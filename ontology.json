  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Actor is a subclass of Resource, which refers to all the agents that are responsible of activities."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://bim.aalto.fi/DiCtion/Ontology/Resource"
  } ]
}, {
